<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <css src="SM_Catalog::css/jssocials.css"/>
        <css src="SM_Catalog::css/jssocials-theme-flat.css"/>
    </head>
    <body>
        <referenceBlock name="product.info.addtocart.bundle">
            <block class="Magento\Catalog\Block\Product\View"
                   name="product.buynow"
                   template="SM_Catalog::buynow-view.phtml"
                   before="-"/>
        </referenceBlock>
        <referenceContainer name="product.info.media">
            <!--social share-->
            <block class="Magento\Catalog\Block\Product\View" name="social-sharing" template="SM_Catalog::social/socialshare.phtml" after="-"/>
        </referenceContainer>
    </body>
</page>
