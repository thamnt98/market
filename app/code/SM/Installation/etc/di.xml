<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:ObjectManager/etc/config.xsd">
    <type name="Magento\Quote\Model\Quote\Item">
        <plugin name="sm_installation_quote_item_plugin"
                type="SM\Installation\Plugin\MagentoQuote\Model\Quote\Item" />
    </type>

    <type name="Magento\Checkout\Model\CompositeConfigProvider">
        <plugin name="sm_installation_checkout_config_plugin"
                type="SM\Installation\Plugin\MagentoCheckout\Model\CompositeConfigProvider" />
    </type>

    <preference for="SM\Installation\Api\Data\InstallationServiceInterface"
                type="SM\Installation\Model\InstallationService"/>
    <preference for="SM\Installation\Api\InstallationRepositoryInterface"
                type="SM\Installation\Model\InstallationRepository"/>
</config>
