<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <transmart_general>
            <header>
                <transmart_app_url>#</transmart_app_url>
            </header>
        </transmart_general>
        <catalog>
            <frontend>
                <default_category_layout>2columns-left</default_category_layout>
                <default_product_layout>1column</default_product_layout>
