<?xml version="1.0" ?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Api/etc/extension_attributes.xsd">
	<extension_attributes for="Magento\Customer\Api\Data\AddressInterface">
		<attribute code="use_store_pick_up" type="string"/>
	</extension_attributes>
    <extension_attributes for="Magento\Quote\Api\Data\CartItemInterface">
        <attribute code="item_data" type="SM\Checkout\Api\Data\CartItemDataInterface"/>
        <attribute code="cart_message" type="SM\Checkout\Api\Data\CartMessage"/>
        <attribute code="apply_voucher" type="string"/>
    </extension_attributes>
    <extension_attributes for="Magento\Quote\Api\Data\TotalsInterface">
        <attribute code="apply_voucher" type="SM\Checkout\Api\Data\CheckoutWeb\VoucherInterface[]"/>
    </extension_attributes>
</config>
