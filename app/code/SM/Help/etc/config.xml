<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <sm_help>
            <seo>
                <base_route>help</base_route>
                <topic_url_suffix>.html</topic_url_suffix>
                <question_url_suffix>.html</question_url_suffix>
            </seo>
            <send_help_email>
                <template>sm_help_email_template</template>
                <sender>general</sender>
            </send_help_email>
            <main_page>
                <contact_us_delivery>9</contact_us_delivery>
            </main_page>
            <payment_page>
                <terms_conditions_mobile>56</terms_conditions_mobile>
            </payment_page>
        </sm_help>
    </default>
</config>
