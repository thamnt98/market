<?xml version="1.0" ?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
	<acl>
		<resources>
			<resource id="Magento_Backend::admin">
				<resource id="SM_FileManagement::File" sortOrder="100" title="File Management">
					<resource id="SM_FileManagement::File_save" sortOrder="10" title="Save File"/>
					<resource id="SM_FileManagement::File_delete" sortOrder="20" title="Delete File"/>
					<resource id="SM_FileManagement::File_update" sortOrder="30" title="Update File"/>
					<resource id="SM_FileManagement::File_view" sortOrder="40" title="View File"/>
				</resource>
			</resource>
		</resources>
	</acl>
</config>
