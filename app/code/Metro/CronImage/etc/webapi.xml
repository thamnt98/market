<?xml version="1.0" ?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
	<route url="/V1/metro-cronimage/cronimage" method="GET">
		<service class="Metro\CronImage\Api\CronImageManagementInterface" method="getCronImage"/>
		<resources>
			<resource ref="anonymous"/>
		</resources>
	</route>

    <route url="/V1/metro-cronimage/cronimagedata" method="GET">
		<service class="Metro\CronImage\Api\CronImageManagementInterface" method="getCronImageData"/>
		<resources>
			<resource ref="anonymous"/>
		</resources>
	</route>
</routes>