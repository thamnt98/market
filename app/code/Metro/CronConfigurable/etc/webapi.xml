<?xml version="1.0" ?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
	<route url="/V1/metro-cronconfigurable/cronconfigurable" method="GET">
		<service class="Metro\CronConfigurable\Api\CronConfigurableManagementInterface" method="getCronConfigurable"/>
		<resources>
			<resource ref="anonymous"/>
		</resources>
	</route>

    <route url="/V1/metro-cronconfigurable/cronconfigurabledata" method="GET">
		<service class="Metro\CronConfigurable\Api\CronConfigurableManagementInterface" method="getCronConfigurableData"/>
		<resources>
			<resource ref="anonymous"/>
		</resources>
	</route>
</routes>