<?xml version="1.0" ?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
	<route url="/V1/metro-cronproduct/cronproduct" method="GET">
		<service class="Metro\CronProduct\Api\CronProductManagementInterface" method="getCronProduct"/>
		<resources>
			<resource ref="anonymous"/>
		</resources>
	</route>
	
	<route url="/V1/metro-cronproduct/cronproductdata" method="GET">
		<service class="Metro\CronProduct\Api\CronProductManagementInterface" method="getCronProductData"/>
		<resources>
			<resource ref="anonymous"/>
		</resources>
	</route>
</routes>