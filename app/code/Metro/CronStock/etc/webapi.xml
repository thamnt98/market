<?xml version="1.0" ?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
	<route url="/V1/metro-cronstock/cronstock" method="GET">
		<service class="Metro\CronStock\Api\CronStockManagementInterface" method="getCronStock"/>
		<resources>
			<resource ref="anonymous"/>
		</resources>
	</route>

	<route url="/V1/metro-cronstock/cronstockdata" method="GET">
		<service class="Metro\CronStock\Api\CronStockManagementInterface" method="getCronStockData"/>
		<resources>
			<resource ref="anonymous"/>
		</resources>
	</route>
</routes>