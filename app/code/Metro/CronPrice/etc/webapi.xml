<?xml version="1.0" ?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
	<route url="/V1/metro-cronprice/cronprice" method="GET">
		<service class="Metro\CronPrice\Api\CronPriceManagementInterface" method="getCronPrice"/>
		<resources>
			<resource ref="anonymous"/>
		</resources>
	</route>

    <route url="/V1/metro-cronprice/cronpricedata" method="GET">
		<service class="Metro\CronPrice\Api\CronPriceManagementInterface" method="getCronPriceData"/>
		<resources>
			<resource ref="anonymous"/>
		</resources>
	</route>
</routes>