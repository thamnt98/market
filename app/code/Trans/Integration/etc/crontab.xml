<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Cron:etc/crontab.xsd">
    <group id="integration_table_cleaner">
        <!-- cron integration_table_cleaner. executed at 03.00 every day -->
        <job name="integration_table_cleaner" instance="Trans\Integration\Cron\IntegrationTableCleaner" method="execute">
            <schedule>0 3 * * *</schedule> 
        <!-- end cron integration_table_cleaner. -->
        </job>
    </group>
</config>
