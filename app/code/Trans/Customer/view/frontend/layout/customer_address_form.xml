<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="customer_account"/>
    <body>
        <referenceContainer name="content">
            <referenceBlock name="customer_address_edit">
                <action method="setTemplate">
                    <argument name="template" xsi:type="string">Trans_Customer::address/edit.phtml</argument>
                </action>
                <arguments>
                    <argument name="view_model" xsi:type="object">Trans\Customer\ViewModel\CustomerAddress</argument>
                </arguments>
            </referenceBlock>
        </referenceContainer>
    </body>
</page>
