<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Cron:etc/crontab.xsd">
    <group id="ims_stores">

        <job name="integration_ims_get_store_update" instance="Trans\IntegrationEntity\Cron\Ims\Get\StoreUpdate" method="execute">
            <schedule>*/5 * * * *</schedule>
        </job>
        <job name="integration_ims_save_store" instance="Trans\IntegrationEntity\Cron\Ims\Save\StoreSave" method="execute">
            <schedule>* 16 * * *</schedule>
        </job>
    </group>
    <group id="assign_sources">
        <job name="assign_ready_sources" instance="Trans\IntegrationEntity\Cron\Ims\Save\AssignSource" method="execute">
            <schedule>* 16 * * *</schedule>
        </job>
    </group>
    <group id="pim_product_attribute">
        <job name="integration_pim_check_product_attribute" instance="Trans\IntegrationEntity\Cron\Pim\Check\ProductAttribute" method="execute">
            <schedule>*/5 * * * *</schedule>
        </job>
        <job name="integration_pim_get_product_attribute" instance="Trans\IntegrationEntity\Cron\Pim\Get\ProductAttribute" method="execute">
            <schedule>*/5 * * * *</schedule>
        </job>
        <job name="integration_pim_save_product_attribute" instance="Trans\IntegrationEntity\Cron\Pim\Save\ProductAttribute" method="execute">
            <schedule>* 18 * * *</schedule>
        </job>
    </group>
    <group id="pim_product_attribute_set">
        <job name="integration_pim_check_product_attribute_set" instance="Trans\IntegrationEntity\Cron\Pim\Check\ProductAttributeSet" method="execute">
            <schedule>*/5 * * * *</schedule>
        </job>
        <job name="integration_pim_get_product_attribute_set" instance="Trans\IntegrationEntity\Cron\Pim\Get\ProductAttributeSet" method="execute">
            <schedule>*/5 * * * *</schedule>
        </job>
        <job name="integration_pim_save_product_attribute_set" instance="Trans\IntegrationEntity\Cron\Pim\Save\ProductAttributeSet" method="execute">
            <schedule>* 19 * * *</schedule>
        </job>
    </group>
</config>
