<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Cron:etc/crontab.xsd">
   
    <group id="pim_brand">
        <job name="integration_pim_check_brand" instance="Trans\IntegrationBrand\Cron\Pim\Check\Brand" method="execute">
            <schedule>*/5 * * * *</schedule>
        </job>
         <job name="integration_pim_get_brand" instance="Trans\IntegrationBrand\Cron\Pim\Get\Brand" method="execute">
            <schedule>*/5 * * * *</schedule>
        </job>
        <job name="integration_pim_save_brand" instance="Trans\IntegrationBrand\Cron\Pim\Save\Brand" method="execute">
            <schedule>* * * * *</schedule>
        </job>
    </group>
</config>
