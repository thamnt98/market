<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Event/etc/events.xsd">
    <event name="trans_customer_save_email_after">
        <observer name="trans_customer_save_email_after" instance="Trans\CustomerMyProfile\Observer\CustomerSaveEmailAfter"/>
    </event>
    <event name="trans_customer_save_telephone_after">
        <observer name="trans_customer_save_telephone_after" instance="Trans\CustomerMyProfile\Observer\CustomerSaveTelephoneAfter"/>
    </event>
    <event name="customer_account_edited">
        <observer name="trans_customer_account_edited_information" instance="Trans\CustomerMyProfile\Observer\CustomerEditedInformation"/>
    </event>
    <event name="trans_customer_change_password_after">
        <observer name="trans_customer_change_password_after" instance="Trans\CustomerMyProfile\Observer\CustomerChangePasswordAfter"/>
    </event>
</config>
