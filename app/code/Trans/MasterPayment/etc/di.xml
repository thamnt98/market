<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:ObjectManager/etc/config.xsd">
    <!-- Logger Start -->
    <type name="Trans\MasterPayment\Logger\Handler">
        <arguments>
            <argument name="filesystem" xsi:type="object">Magento\Framework\Filesystem\Driver\File</argument>
        </arguments>
    </type>

    <type name="Trans\MasterPayment\Logger\Logger">
        <arguments>
            <argument name="name" xsi:type="string">sprintlog</argument>
            <argument name="handlers"  xsi:type="array">
                <item name="system" xsi:type="object">Trans\MasterPayment\Logger\Handler</item>
            </argument>
        </arguments>
    </type>
    <!-- Logger End -->

    <preference for="Trans\MasterPayment\Api\Data\MasterPaymentInterface" type="Trans\MasterPayment\Model\MasterPayment" />
    <preference for="Trans\MasterPayment\Api\MasterPaymentRepositoryInterface" type="Trans\MasterPayment\Model\MasterPaymentRepository" />
    <preference for="Trans\MasterPayment\Api\PaymentInterface" type="Trans\MasterPayment\Model\Payment" />

    <preference for="Trans\MasterPayment\Api\Data\MasterPaymentMatrixAdjustmentInterface" type="Trans\MasterPayment\Model\MasterPaymentMatrixAdjustment" />
    <preference for="Trans\MasterPayment\Api\MasterPaymentMatrixAdjustmentRepositoryInterface" type="Trans\MasterPayment\Model\MasterPaymentMatrixAdjustmentRepository" />
</config>