<schema xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Setup/Declaration/Schema/etc/schema.xsd">
  <table name="trans_mepay_pull_status_config" resource="default" engine="innodb" comment="Trans Mepay pull status cron configuration">
    <column xsi:type="int" name="row_id" padding="6" unsigned="true" nullable="false" identity="true" comment="row ID"/>
    <column xsi:type="varchar" length="255" name="config_name" nullable="false" comment="config name"/>
    <column xsi:type="int" name="config_offset" padding="6" unsigned="true" default="0" nullable="false" comment="config offset"/>
    <column xsi:type="int" name="config_limit" padding="6" unsigned="true" default="1" nullable="false" comment="config"/>
    <column xsi:type="timestamp" name="updated_at" comment="config updated_at"/>
    <constraint xsi:type="primary" referenceId="PRIMARY">
      <column name="row_id"/>
    </constraint>
  </table>
</schema>