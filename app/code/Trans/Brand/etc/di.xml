<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:ObjectManager/etc/config.xsd">
    <preference for="Trans\Brand\Api\Data\BrandInterface" type="Trans\Brand\Model\Brands" />

    <!-- Override Amasty Brand -->
    <preference for="Amasty\ShopbyBase\Model\OptionSetting" type="Trans\Brand\Model\AmastyBrand" />
    <preference for="Trans\Brand\Api\Data\AmastyBrandInterface" type="Trans\Brand\Model\AmastyBrand" />

    <!-- Override Amasty Brand -->
    <virtualType name="BrandGridFilterPool" type="Magento\Framework\View\Element\UiComponent\DataProvider\FilterPool">
        <arguments>
            <argument name="appliers" xsi:type="array">
                <item name="regular" xsi:type="object">Magento\Framework\View\Element\UiComponent\DataProvider\RegularFilter</item>
                <item name="fulltext" xsi:type="object">Magento\Framework\View\Element\UiComponent\DataProvider\FulltextFilter</item>
            </argument>
        </arguments>
    </virtualType>
    <virtualType name="BrandGridDataProvider" type="Magento\Framework\View\Element\UiComponent\DataProvider\DataProvider">
        <arguments>
            <argument name="collection" xsi:type="object" shared="false">Trans\Brand\Model\ResourceModel\Brand\Collection</argument>
            <argument name="filterPool" xsi:type="object" shared="false">BrandGridFilterPool</argument>
        </arguments>
    </virtualType>
    <virtualType name="Trans\Brand\Model\ResourceModel\Brand\Grid\Collection" type="Magento\Framework\View\Element\UiComponent\DataProvider\SearchResult">
        <arguments>
            <argument name="mainTable" xsi:type="string">trans_brand</argument>
            <argument name="resourceModel" xsi:type="string">Trans\Brand\Model\ResourceModel\Brand</argument>
        </arguments>
    </virtualType>
    <type name="Magento\Framework\View\Element\UiComponent\DataProvider\CollectionFactory">
        <arguments>
            <argument name="collections" xsi:type="array">
                <item name="brands_index_listing_data_source" xsi:type="string">Trans\Brand\Model\ResourceModel\Brand\Grid\Collection</item>
            </argument>
        </arguments>
    </type>
    <type name="Trans\Brand\Model\ResourceModel\Brand\Grid\Collection">
        <arguments>
            <argument name="mainTable" xsi:type="string">trans_brand</argument>
            <argument name="eventPrefix" xsi:type="string">trans_brand_grid_collection</argument>
            <argument name="eventObject" xsi:type="string">trans_brand_grid_collection</argument>
            <argument name="resourceModel" xsi:type="string">Trans\Brand\Model\ResourceModel\Brand</argument>
        </arguments>
    </type>
</config>